  <xacro:include filename="$(find uclv_wsg50_simulation)/urdf/wsg50.xacro" />
  <xacro:wsg50 base_link="${base_link}" prefix_finger_left="${prefix_finger_left}" prefix_finger_right="${prefix_finger_right}"
